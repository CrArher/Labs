#include<stdio.h>
#pragma warning(disable : 4996)

void func();
int main()
{
    func();

    return 0;
}

void func()
{
    float s = 1, z = 1, x;
    int i, n;

    printf("x=");
    scanf("%f\n", &x);

    for (i = 1, n = 1; i <= 10; i++, n++)
    {
        z = (z * x * x) / (2 * n * (2 * n - 1));
        s += z;
        printf("s=%f\n", s);
    }
}
